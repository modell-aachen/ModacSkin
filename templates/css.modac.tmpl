%IF{ '$ WEBHEADERBGCOLOR' then='#patternTopBarContents { background-color:%WEBHEADERBGCOLOR%;}'}%
.patternBookView {
	border-color:%WEBBGCOLOR%;
}
.patternPreviewPage #patternMain {
